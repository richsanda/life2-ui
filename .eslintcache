={() => { }}\n                />\n            </Card.Body>\n        </Accordion.Collapse>\n    </Card>);\n}\n\nconst CreateNoteDropdown = (props) => {\n\n    const { addNote } = props;\n\n    return (<Dropdown as={ButtonGroup}>\n        <Button style={{ fontSize: \".75em\" }} size=\"sm\" onClick={() => addNote(\"whatever\")}>+ note</Button>\n        <Dropdown.Toggle split style={{ fontSize: \".75em\" }} id=\"dropdown-basic\" />\n        <Dropdown.Menu>\n            <Dropdown.Item size=\"sm\" style={{ fontSize: \".75em\" }} onClick={() => addNote(\"$$receipt\")}>receipt</Dropdown.Item>\n            <Dropdown.Item size=\"sm\" style={{ fontSize: \".75em\" }} onClick={() => addNote(\"$$journal\")}>journal</Dropdown.Item>\n            <Dropdown.Item size=\"sm\" style={{ fontSize: \".75em\" }} onClick={() => addNote(\"$$song\")}>song</Dropdown.Item>\n        </Dropdown.Menu>\n    </Dropdown>)\n}\n\nconst ArtifactModal = (props) => {\n\n    const [personOptions, setPersonOptions] = useState<Person[]>([]);\n    const [troveOptions, setTroveOptions] = useState<Trove[]>([]);\n  \n    useEffect(() => {\n      ArtifactAPI.persons().then((response) => {\n        setPersonOptions(response);\n      })\n      ArtifactAPI.troves().then((response) => {\n        setTroveOptions(response);\n      })\n    }, [])\n\n    const [notes, setNotes] = useState<string[]>([]);\n    const [activeKey, setActiveKey] = useState<number>(0);\n\n    const addNote = (content) => {\n        setNotes([...notes, content]); setActiveKey(notes.length);\n    }\n\n    const removeNote = (removeKey) => {\n        setNotes(notes.filter((n, i) => i != removeKey));\n        setActiveKey(-1);\n    }\n\n    const toggleNote = (toggleKey) => {\n        if (activeKey === toggleKey) {\n            setActiveKey(-1);\n        } else {\n            setActiveKey(toggleKey);\n        }\n    }\n\n    const {\n        show,\n        artifact,\n        commentary,\n        relativeKeys,\n        relativeKeyIndex,\n        handleSelect,\n        handleChange,\n        handleClose } = props;\n\n    return (\n        <Modal size=\"lg\" show={show} onHide={handleClose}>\n            <Modal.Header>\n                <Modal.Title>\n                    {prettyDate(artifact.when)} &#8212; {artifact.title}\n                </Modal.Title>\n                <Button style={{ fontSize: \".75em\" }} variant=\"primary\" onClick={handleClose}>\n                    save\n                </Button>\n            </Modal.Header>\n            <Modal.Body>\n                <MDBContainer>\n                    <MDBRow>\n                        <MDBCol md=\"8\">\n                            {\n                                (!!!artifact.types) ? (\n\n                                    <></>\n\n                                ) : (artifact.types.indexOf(\"email\") > -1) ? (\n\n                                    <EmailFeature artifact={artifact} />\n\n                                ) : (artifact.types.indexOf(\"note\") > -1) ?\n\n                                    (<ArtifactImageFeature\n                                        artifact={artifact}\n                                        relativeKeys={relativeKeys}\n                                        relativeKeyIndex={relativeKeyIndex}\n                                        handleSelect={handleSelect}\n                                    />\n\n                                    ) :\n\n                                    <></>\n                            }\n                        </MDBCol>\n                        <MDBCol md=\"4\">\n                            <MDBRow>\n                                <CommentaryBox\n                                    value={commentary}\n                                    onChange={handleChange}\n                                    personOptions={personOptions}\n                                    onAdd={() => { }}\n                                />\n                            </MDBRow>\n                           \n                            <MDBRow>\n                                <Accordion activeKey={\"\" + activeKey} defaultActiveKey={null}>\n\n                                    {notes.map((n, i) => {\n\n                                        return (<ArtifactNote\n                                            content={n}\n                                            index={i}\n                                            toggleNote={toggleNote}\n                                            removeNote={removeNote}\n                                            personOptions={personOptions}\n                                        />);\n                                    })}\n                                </Accordion>\n                            </MDBRow>\n                            <MDBRow style={{padding: \"8px\"}}>\n                                <CreateNoteDropdown addNote={addNote} />\n                            </MDBRow>\n                        </MDBCol>\n                    </MDBRow>\n                </MDBContainer>\n            </Modal.Body>\n        </Modal >\n    )\n}\n\nexport default ArtifactModal;","/Users/sg0307091/checkouts/life2-ui/src/components/modal/ArtifactImageFeature.tsx",[],"/Users/sg0307091/checkouts/life2-ui/src/components/modal/ArtifactImageDisplay.tsx",[],"/Users/sg0307091/checkouts/life2-ui/src/components/modal/EmailFeature.tsx",[],"/Users/sg0307091/checkouts/life2-ui/src/components/commentary/CommentaryBox.tsx",["184","185","186","187"],"/Users/sg0307091/checkouts/life2-ui/src/components/Life2/SearchPane.tsx",["188","189"],{"ruleId":"190","replacedBy":"191"},{"ruleId":"192","replacedBy":"193"},{"ruleId":"194","severity":1,"message":"195","line":1,"column":46,"nodeType":"196","messageId":"197","endLine":1,"endColumn":55},{"ruleId":"194","severity":1,"message":"198","line":4,"column":25,"nodeType":"196","messageId":"197","endLine":4,"endColumn":35},{"ruleId":"194","severity":1,"message":"199","line":8,"column":8,"nodeType":"196","messageId":"